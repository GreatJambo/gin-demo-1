LA;AAMA,IAAA,QANA,sBAMA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EADA,CAEA;;AACA,WAAA,EAAA,CAAA,IAAA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,IAAA,CAAA,SAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EARA,CASA;;AACA,MAAA,WAAA,CAAA,QAAA,CAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,cAAA,CAAA,GAAA,CAAA,cAAA,CAAA,UAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,WAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA,UAAA,cAAA,CAAA,GAAA,CAAA,cAAA,CAAA,SAAA,EAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAFA,CAGA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AACA,SALA;AAMA,OATA,EASA,KATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,UAAA,KAAA,EAAA,QADA;AAEA,UAAA,OAAA,EAAA,QAFA;AAGA,UAAA,KAAA,EAAA;AAHA,SAAA;AAKA,OAjBA;AAmBA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA;AApCA;AA7BA,CAAA","sourcesContent":["<template>\n  <div class=\"register\">\n    <b-row class=\"mt-5\">\n      <b-col\n        md=\"8\"\n        offset-md=\"2\"\n        lg=\"6\"\n        offset-lg=\"3\"\n      >\n        <b-card title=\"注册\">\n          <b-form>\n            <b-form-group label=\"姓名\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.name.$model\"\n                type=\"text\"\n                required\n                placeholder=\"输入用户名称\"\n                :state=\"validateState('name')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('name')\">\n                用户名长度必须是2-12位\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group label=\"电话\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.telephone.$model\"\n                type=\"number\"\n                required\n                placeholder=\"输入手机号码\"\n                :state=\"validateState('telephone')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('telephone')\">\n                手机号不符合要求\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group label=\"密码\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.password.$model\"\n                type=\"password\"\n                required\n                placeholder=\"输入用户密码\"\n                :state=\"validateState('password')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('password')\">\n                密码长度必须在6-18之间\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group>\n              <b-button\n                @click=\"register\"\n                variant=\"outline-primary\"\n                block=\"\"\n              >注册</b-button>\n            </b-form-group>\n          </b-form>\n        </b-card>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n\n<script>\nimport { required, minLength, maxLength } from 'vuelidate/lib/validators';\n\nimport customValidator from '@/helper/validator';\nimport storageService from '@/service/storageService';\nimport userService from '@/service/userService';\n\nexport default {\n  data() {\n    return {\n      user: {\n        name: '',\n        telephone: '',\n        password: '',\n      },\n      validation: null,\n    };\n  },\n  validations: {\n    user: {\n      name: {\n        required,\n        minLength: minLength(2),\n        maxLength: maxLength(12),\n      },\n      telephone: {\n        required,\n        telephone: customValidator.telephoneValidator,\n      },\n      password: {\n        required,\n        minLength: minLength(6),\n        maxLength: maxLength(18),\n      },\n    },\n  },\n  methods: {\n    validateState(name) {\n      // 这里是es6的 析构赋值\n      const { $dirty, $error } = this.$v.user[name];\n      return $dirty ? !$error : null;\n    },\n    register() {\n      console.log('正在请求API');\n      // 验证数据\n      this.$v.user.$touch();\n      if (this.$v.user.$anyError) {\n        console.log('API拒绝请求');\n        return;\n      }\n      console.log('正在发送请求');\n      // 请求api\n      userService.register(this.user).then((res) => {\n        // 保存token\n        storageService.set(storageService.USER_TOKEN, res.data.data.token);\n        userService.info((response) => {\n          // 保存用户信息\n          storageService.set(storageService.USER_INFO, JSON.stringify(response.data.data.user));\n          // 跳转主页\n          this.$router.replace({ name: 'Home' });\n        });\n      }).catch((err) => {\n        console.log(err);\n        console.log('回调失败');\n        this.$bvToast.toast(err.response.data.msg, {\n          title: '数据验证错误',\n          variant: 'danger',\n          solid: true,\n        });\n      });\n\n      console.log('register');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"],"sourceRoot":"src/views/register"}]}