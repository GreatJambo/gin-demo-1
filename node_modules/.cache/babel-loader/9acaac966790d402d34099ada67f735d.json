CA;AACA,OANA,CAQA;;;AACA,WAAA,SAAA,CAAA,KAAA,IAAA,EAAA,IAAA,CAAA,YAAA;AACA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAHA,EAGA,KAHA,CAGA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,UAAA,KAAA,EAAA,QADA;AAEA,UAAA,OAAA,EAAA,QAFA;AAGA,UAAA,KAAA,EAAA;AAHA,SAAA;AAKA,OATA;AAWA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA;AA5BA;AAvBA,CAAA","sourcesContent":["<template>\n  <div class=\"login\">\n    <b-row class=\"mt-5\">\n      <b-col\n        md=\"8\"\n        offset-md=\"2\"\n        lg=\"6\"\n        offset-lg=\"3\"\n      >\n        <b-card title=\"登录\">\n          <b-form>\n\n            <b-form-group label=\"电话\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.telephone.$model\"\n                type=\"number\"\n                required\n                placeholder=\"输入手机号码\"\n                :state=\"validateState('telephone')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('telephone')\">\n                手机号不符合要求\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group label=\"密码\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.password.$model\"\n                type=\"password\"\n                required\n                placeholder=\"输入用户密码\"\n                :state=\"validateState('password')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('password')\">\n                密码长度必须在6-18之间\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group>\n              <b-button\n                @click=\"login\"\n                variant=\"outline-primary\"\n                block=\"\"\n              >登录</b-button>\n            </b-form-group>\n          </b-form>\n        </b-card>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n\n<script>\nimport { required, minLength, maxLength } from 'vuelidate/lib/validators';\n\nimport customValidator from '@/helper/validator';\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      user: {\n        telephone: '',\n        password: '',\n      },\n      validation: null,\n    };\n  },\n  validations: {\n    user: {\n      telephone: {\n        required,\n        telephone: customValidator.telephoneValidator,\n      },\n      password: {\n        required,\n        minLength: minLength(6),\n        maxLength: maxLength(18),\n      },\n    },\n  },\n  methods: {\n    ...mapActions('userModule', { userlogin: 'login' }),\n    validateState(name) {\n      // 这里是es6的 析构赋值\n      const { $dirty, $error } = this.$v.user[name];\n      return $dirty ? !$error : null;\n    },\n    login() {\n      // 验证数据\n      this.$v.user.$touch();\n      if (this.$v.user.$anyError) {\n        console.log('API拒绝请求');\n        return;\n      }\n\n      // 请求api\n      this.userlogin(this.user).then(() => {\n        // 跳转首页\n        this.$router.replace({ name: 'Home' });\n      }).catch((err) => {\n        this.$bvToast.toast(err.data.data.msg, {\n          title: '数据验证错误',\n          variant: 'danger',\n          solid: true,\n        });\n      });\n\n      console.log('login');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"],"sourceRoot":"src/views/login"}]}