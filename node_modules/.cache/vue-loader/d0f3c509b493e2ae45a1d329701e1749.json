{"remainingRequest":"/home/pan/Work/src/html/jkdev-vue/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/pan/Work/src/html/jkdev-vue/src/views/register/Register.vue?vue&type=script&lang=js&","dependencies":[{"path":"/home/pan/Work/src/html/jkdev-vue/src/views/register/Register.vue","mtime":1588256643055},{"path":"/home/pan/Work/src/html/jkdev-vue/node_modules/cache-loader/dist/cjs.js","mtime":1587271952385},{"path":"/home/pan/Work/src/html/jkdev-vue/node_modules/babel-loader/lib/index.js","mtime":1587271952408},{"path":"/home/pan/Work/src/html/jkdev-vue/node_modules/cache-loader/dist/cjs.js","mtime":1587271952385},{"path":"/home/pan/Work/src/html/jkdev-vue/node_modules/vue-loader/lib/index.js","mtime":1587271961065}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Register.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoEA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"Register.vue","sourceRoot":"src/views/register","sourcesContent":["<template>\n  <div class=\"register\">\n    <b-row class=\"mt-5\">\n      <b-col\n        md=\"8\"\n        offset-md=\"2\"\n        lg=\"6\"\n        offset-lg=\"3\"\n      >\n        <b-card title=\"注册\">\n          <b-form>\n            <b-form-group label=\"姓名\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.name.$model\"\n                type=\"text\"\n                required\n                placeholder=\"输入用户名称\"\n                :state=\"validateState('name')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('name')\">\n                用户名长度必须是2-12位\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group label=\"电话\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.telephone.$model\"\n                type=\"number\"\n                required\n                placeholder=\"输入手机号码\"\n                :state=\"validateState('telephone')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('telephone')\">\n                手机号不符合要求\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group label=\"密码\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"$v.user.password.$model\"\n                type=\"password\"\n                required\n                placeholder=\"输入用户密码\"\n                :state=\"validateState('password')\"\n              ></b-form-input>\n              <b-form-invalid-feedback :state=\"validateState('password')\">\n                密码长度必须在6-18之间\n              </b-form-invalid-feedback>\n            </b-form-group>\n\n            <b-form-group>\n              <b-button\n                @click=\"register\"\n                variant=\"outline-primary\"\n                block=\"\"\n              >注册</b-button>\n            </b-form-group>\n          </b-form>\n        </b-card>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n\n<script>\nimport { required, minLength, maxLength } from 'vuelidate/lib/validators';\n\nimport customValidator from '@/helper/validator';\nimport storageService from '@/service/storageService';\nimport userService from '@/service/userService';\n\nexport default {\n  data() {\n    return {\n      user: {\n        name: '',\n        telephone: '',\n        password: '',\n      },\n      validation: null,\n    };\n  },\n  validations: {\n    user: {\n      name: {\n        required,\n        minLength: minLength(2),\n        maxLength: maxLength(12),\n      },\n      telephone: {\n        required,\n        telephone: customValidator.telephoneValidator,\n      },\n      password: {\n        required,\n        minLength: minLength(6),\n        maxLength: maxLength(18),\n      },\n    },\n  },\n  methods: {\n    validateState(name) {\n      // 这里是es6的 析构赋值\n      const { $dirty, $error } = this.$v.user[name];\n      return $dirty ? !$error : null;\n    },\n    register() {\n      console.log('正在请求API');\n      // 验证数据\n      this.$v.user.$touch();\n      if (this.$v.user.$anyError) {\n        console.log('API拒绝请求');\n        return;\n      }\n      console.log('正在发送请求');\n      // 请求api\n      userService.register(this.user).then((res) => {\n        // 保存token\n        storageService.set(storageService.USER_TOKEN, res.data.data.token);\n        userService.info((response) => {\n          // 保存用户信息\n          storageService.set(storageService.USER_INFO, JSON.stringify(response.data.data.user));\n          // 跳转主页\n          this.$router.replace({ name: 'Home' });\n        });\n      }).catch((err) => {\n        console.log(err);\n        console.log('回调失败');\n        this.$bvToast.toast(err.response.data.msg, {\n          title: '数据验证错误',\n          variant: 'danger',\n          solid: true,\n        });\n      });\n\n      console.log('register');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}